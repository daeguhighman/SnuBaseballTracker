{
  "name": "snu_baseball-tracker",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && npm run sentry:sourcemaps",
    "postbuild": "cp -R src/seed/csv dist/src/seed/csv",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:test": "cross-env NODE_ENV=test nest start --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:cov": "jest --coverage --config jest.config.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.ts",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.config.ts",
    "typeorm": "typeorm-ts-node-esm",
    "seed:truncate": "ts-node -r tsconfig-paths/register src/seed/seed-truncate.ts",
    "seed:umpire": "ts-node -r tsconfig-paths/register src/seed/seed-umpire.ts",
    "seed:lineup": "ts-node -r tsconfig-paths/register src/seed/seed-lineup.ts",
    "seed:init": "ts-node -r tsconfig-paths/register src/seed/seed-init.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./data-source.ts",
    "migration:run": "npx typeorm migration:run -d dist/data-source.js",
    "migration:revert": "npx typeorm migration:revert -d dist/data-source.js",
    "seed:prod": "node dist/src/seed/seed-real.js",
    "seed:dev": "ts-node -r tsconfig-paths/register src/seed/seed-real.ts",
    "seed:pt": "ts-node -r tsconfig-paths/register src/seed/seed-pt.ts",
    "seed:bracket": "ts-node -r tsconfig-paths/register src/seed/seed-bracket.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org daeguhighman --project node-nestjs ./dist && sentry-cli sourcemaps upload --org daeguhighman --project node-nestjs ./dist",
    "gen:swagger": "ts-node --project tsconfig.json scripts/swagger.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^11.0.0",
    "@sentry/cli": "^2.45.0",
    "@sentry/nestjs": "^9.17.0",
    "cache-manager": "^6.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.5.0",
    "mysql2": "^3.14.0",
    "nodemailer": "^6.10.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.15",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}